{:lein         true
 :builds       {:app {:target     :browser
                      :output-dir "resources/public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [diceandclocks.core]}}
                      :devtools   {:after-load diceandclocks.core/mount-root}}}
 :dev-http     {3000 {:root    "resources/public"
                      :handler diceandclocks.handler/app}}}
